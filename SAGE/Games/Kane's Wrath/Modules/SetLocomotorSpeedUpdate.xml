<?xml version="1.0" encoding="utf-8" ?>
<AssetDefinition xmlns="uri:thundermods.net:SAGE:WrathEdXML:AssetDefinition">

	<EnumAsset
		id="SetLocomotorSpeedUpdateType">
		<Entry>INVALID</Entry>
		<Entry>IN_RANGE_OBJECT_FILTER</Entry>
	</EnumAsset>

	<Asset
		id="SetLocomotorSpeedUpdateModuleData">
		<EntryInheritance
			id="base"
			AssetType="UpdateModuleData" />
		<Entry
			id="Type"
			AssetType="SetLocomotorSpeedUpdateType"
			IsAttribute="true"
			Default="INVALID" />
		<Entry
			id="LocomotorSpeedToSet"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0.0" />
		<Entry
			id="LocomotorDeltaPerUpdate"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0.0" />
		<Entry
			id="OtherScaredScanRangeMultiplier"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0.0" />
		<Entry
			id="ObjectFilterRange"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0.0" />
		<Entry
			id="UpdatePeriod"
			AssetType="Time"
			IsAttribute="true"
			Default="1.0s" />
		<Entry
			id="VisionArc"
			AssetType="Angle"
			IsAttribute="true"
			Default="180d" />
		<EntryRelocation
			id="ObjectFilter"
			AssetType="ObjectFilter" />
	</Asset>

</AssetDefinition>