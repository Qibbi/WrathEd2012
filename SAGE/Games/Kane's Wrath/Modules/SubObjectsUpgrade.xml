<?xml version="1.0" encoding="utf-8" ?>
<AssetDefinition xmlns="uri:thundermods.net:SAGE:WrathEdXML:AssetDefinition">

	<Asset
		id="SubObjectsUpgradeModuleData">
		<EntryInheritance
			id="base"
			AssetType="UpgradeModuleData" />
		<EntryList
			id="ShowSubObjects"
			AssetType="String"
			IsAttribute="true"
			MinLength="0"
			MaxLength="0" />
		<EntryList
			id="HideSubObjects"
			AssetType="String"
			IsAttribute="true"
			MinLength="0"
			MaxLength="0" />
		<Entry
			id="FadeTimeInSeconds"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0.5" />
		<Entry
			id="WaitBeforeFadeInSeconds"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0.0" />
		<EntryList
			id="ExcludeSubobjects"
			AssetType="String"
			IsAttribute="true"
			MinLength="0"
			MaxLength="0" />
		<Entry
			id="RecolorHouse"
			AssetType="SageBool"
			IsAttribute="true"
			Default="false" />
		<Entry
			id="SkipFadeOnCreate"
			AssetType="SageBool"
			IsAttribute="true"
			Default="false" />
		<Entry
			id="HideSubObjectsOnRemove"
			AssetType="SageBool"
			IsAttribute="true"
			Default="false" />
		<Entry
			id="UnHideSubObjectsOnRemove"
			AssetType="SageBool"
			IsAttribute="true"
			Default="false" />
	</Asset>

</AssetDefinition>