			id="base"
			AssetType="UpdateModuleData" />
		<Entry
			id="Radius"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0" />
		<Entry
			id="MaxIncome"
			AssetType="SageInt"
			IsAttribute="true"
			Default="0" />
		<Entry
			id="IncomeInterval"
			AssetType="SageUnsignedInt"
			IsAttribute="true"
			Default="0xFFFFFFFF" />
