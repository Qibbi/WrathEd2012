			id="UpgradeBonusPercent"
			AssetType="Percentage"
			IsAttribute="true"
			Default="100" />
		<EntryRelocation
			id="ObjectFilter"
			AssetType="ObjectFilter" />
		<Entry
			id="HighPriority"
			AssetType="SageBool"
			IsAttribute="true"
			Default="false" />
		<Entry
			id="Visible"
			AssetType="SageBool"
			IsAttribute="true"
			Default="true" />
		<Entry
			id="fill"
			AssetType="Byte"
			IsVoid="true" />
