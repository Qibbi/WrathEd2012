<?xml version="1.0" encoding="utf-8" ?>
<AssetDefinition xmlns="uri:thundermods.net:SAGE:WrathEdXML:AssetDefinition">

	<Asset
		id="Query">
		<Entry
			id="EMax"
			AssetType="SageInt"
			IsAttribute="true"
			Default="6" />
		<Entry
			id="Value"
			AssetType="SageInt"
			IsAttribute="true"
			Default="-1" />
		<EntryRelocation
			id="ObjectFilter"
			AssetType="ObjectFilter" />
	</Asset>

	<Asset
		id="AutoAbilityBehaviorModuleData">
		<EntryInheritance
			id="base"
			AssetType="UpdateModuleData" />
		<Entry
			id="MaxScanRange"
			AssetType="SageReal"
			IsAttribute="true"
			Default="1" />
		<Entry
			id="MinScanRange"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0" />
		<Entry
			id="Radius"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0" />
		<Entry
			id="IdleSeconds"
			AssetType="Time"
			IsAttribute="true"
			Default="0s" />
		<EntryReference
			id="SpecialAbilityName"
			AssetType="SpecialPowerTemplate"
			IsAttribute="true" />
		<Entry
			id="ForbiddenStatus"
			AssetType="ObjectStatusBitFlags"
			IsAttribute="true" />
		<EntryList
			id="Queries"
			AssetType="Query"
			MinLength="0"
			MaxLength="6" />
		<Entry
			id="StartsActive"
			AssetType="SageBool"
			IsAttribute="true"
			Default="false" />
		<Entry
			id="UseStartPosition"
			AssetType="SageBool"
			IsAttribute="true"
			Default="false" />
		<Entry
			id="AdjustAttackPos"
			AssetType="SageBool"
			IsAttribute="true"
			Default="false" />
		<Entry
			id="AllowSelf"
			AssetType="SageBool"
			IsAttribute="true"
			Default="false" />
	</Asset>

</AssetDefinition>