<?xml version="1.0" encoding="utf-8" ?>
<AssetDefinition xmlns="uri:thundermods.net:SAGE:WrathEdXML:AssetDefinition">

	<Asset
		id="FXParticleDrawBase">
		<EntryInheritance
			id="base"
			AssetType="FXParticleBaseModule" />
	</Asset>

	<Asset
		id="FXParticleDrawGpu">
		<EntryInheritance
			id="base"
			AssetType="FXParticleDrawBase" />
		<Entry
			id="Shader"
			AssetType="String"
			IsAttribute="true"
			Default="GPUParticle.fx" />
		<Entry
			id="FramesPerRow"
			AssetType="SageInt"
			IsAttribute="true"
			Default="1" />
		<Entry
			id="TotalFrames"
			AssetType="SageInt"
			IsAttribute="true"
			Default="1" />
		<EntryReference
			id="DetailTexture"
			AssetType="Texture"
			IsAttribute="true" />
		<Entry
			id="SpeedMultiplier"
			AssetType="SageReal"
			IsAttribute="true"
			Default="1.0" />
		<Entry
			id="GeometryType"
			AssetType="FXParticleSystem_GeometryType"
			IsAttribute="true"
			Default="SIMPLE_QUAD" />
		<Entry
			id="SortParticles"
			AssetType="SageBool"
			IsAttribute="true"
			Default="false" />
		<Entry
			id="fill"
			AssetType="Byte"
			IsVoid="true" />
	</Asset>

	<Asset
		id="RandomSetType">
		<Entry
			id="StartAmplitude"
			AssetType="ClientRandomVariable" />
		<Entry
			id="EndAmplitude"
			AssetType="ClientRandomVariable" />
		<Entry
			id="StartFrequency"
			AssetType="ClientRandomVariable" />
		<Entry
			id="EndFrequency"
			AssetType="ClientRandomVariable" />
		<Entry
			id="StartPhase"
			AssetType="ClientRandomVariable" />
		<Entry
			id="EndPhase"
			AssetType="ClientRandomVariable" />
	</Asset>

	<Asset
		id="FXParticleDrawLightning">
		<EntryInheritance
			id="base"
			AssetType="FXParticleDrawBase" />
		<Entry
			id="StartPoint"
			AssetType="Coord3D" />
		<Entry
			id="EndPoint"
			AssetType="Coord3D" />
		<EntryList
			id="RandomSet"
			AssetType="RandomSetType"
			MinLength="2"
			MaxLength="2" />
	</Asset>

	<Asset
		id="ObjectSetType">
		<Entry
			id="RenderGroup"
			AssetType="String"
			IsAttribute="true" />
		<Entry
			id="NumObjects"
			AssetType="SageInt"
			IsAttribute="true"
			Default="0" />
		<Entry
			id="Percent"
			AssetType="Percentage"
			IsAttribute="true"
			Default="0.0" />
		<Entry
			id="Shader"
			AssetType="FXParticleSystem_ShaderType"
			IsAttribute="true"
			Default="W3D_DIFFUSE" />
	</Asset>

	<Asset
		id="FXParticleDrawRenderObject">
		<EntryInheritance
			id="base"
			AssetType="FXParticleDrawBase" />
		<Entry
			id="SinkRate"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0.0" />
		<EntryList
			id="ObjectSet"
			AssetType="ObjectSetType"
			MinLength="3"
			MaxLength="3" />
		<Entry
			id="MultiRenderObjects"
			AssetType="SageBool"
			IsAttribute="true"
			Default="false" />
		<Entry
			id="SinkOnTerrainCollision"
			AssetType="SageBool"
			IsAttribute="true"
			Default="false" />
		<Entry
			id="fill"
			AssetType="Byte"
			IsVoid="true" />
	</Asset>

	<Asset
		id="FXParticleDrawStreak">
		<EntryInheritance
			id="base"
			AssetType="FXParticleDrawBase" />
	</Asset>

	<Asset
		id="FXParticleDrawSwarm">
		<EntryInheritance
			id="base"
			AssetType="FXParticleDrawBase" />
		<Entry
			id="OpaqueSpeed"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0.0" />
		<Entry
			id="TransparentSpeed"
			AssetType="SageReal"
			IsAttribute="true"
			Default="100.0" />
		<Entry
			id="SpeedStretchAmount"
			AssetType="SageReal"
			IsAttribute="true"
			Default="1.0" />
		<Entry
			id="AttractStrength"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0.0" />
		<EntryReference
			id="EnvironmentTexture"
			AssetType="Texture"
			IsAttribute="true" />
	</Asset>

	<Asset
		id="FXParticleDrawTrail">
		<EntryInheritance
			id="base"
			AssetType="FXParticleDrawBase" />
		<Entry
			id="TrailLife"
			AssetType="SageInt"
			IsAttribute="true"
			Default="30" />
		<Entry
			id="UTile"
			AssetType="SageReal"
			IsAttribute="true"
			Default="1.0" />
		<Entry
			id="VTile"
			AssetType="SageReal"
			IsAttribute="true"
			Default="1.0" />
		<Entry
			id="UScrollRate"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0.0" />
		<Entry
			id="VScrollRate"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0.0" />
	</Asset>

	<Asset
		id="FXParticleDrawModule">
		<EntryChoice
			id="FXParticleDrawModule"
			MinLength="0"
			MaxLength="1">
			<Entry
				id="Default"
				AssetType="FXParticleDrawBase" />
			<Entry
				id="Gpu"
				AssetType="FXParticleDrawGpu" />
			<Entry
				id="Lightning"
				AssetType="FXParticleDrawLightning" />
			<Entry
				id="RenderObject"
				AssetType="FXParticleDrawRenderObject" />
			<Entry
				id="Streak"
				AssetType="FXParticleDrawStreak" />
			<Entry
				id="Swarm"
				AssetType="FXParticleDrawSwarm" />
			<Entry
				id="Trail"
				AssetType="FXParticleDrawTrail" />
		</EntryChoice>
	</Asset>

</AssetDefinition>